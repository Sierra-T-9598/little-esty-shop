<%= render partial: '/layouts/partials/admin_dashboard_links' %><br>
<%= form_with url: merchant_items_path(@merchant.id), method: :post, local: true do |form|  %>
  <%= form.label :name %>
  <%= form.text_field :name, required: :true %>
  <%= form.label :description%>
  <%= form.text_field :description, required: :true %>
  <%= form.label :unit_price%>
  <%= form.text_field :unit_price, required: :true %>
  <%= form.hidden_field :status, value: "disabled" %>
  <%= form.submit "Submit" %>
<% end  %>
